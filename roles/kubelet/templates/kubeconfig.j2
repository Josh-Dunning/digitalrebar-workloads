apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: {{ k8s.kubelet.ca_file }}
    server: https://10.240.0.20:6443
  name: {{ k8s.cluster_name }}
contexts:
- context:
    cluster: {{ k8s.cluster_name }}
    user: kubelet
  name: kubelet
current-context: kubelet
users:
- name: kubelet
  user:
    token: {{ k8s.users["kubelet"]["pass"] }}
